### 1. Glass ###
data(Glass)
rglass <- bootstrap_evtree(Glass, nboots=250)
apply(apply(rglass, 2, as.numeric),2,mean)
save(rglass, file="/home/grubinger/partykit/pkg/evtree/inst/doc/results/glass.RData")


